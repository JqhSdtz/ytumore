<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
	<display-name>ytumore</display-name>
	<welcome-file-list>
		<welcome-file>/jump.jsp</welcome-file>
	</welcome-file-list>
	<session-config>
		<session-timeout>20</session-timeout>
	</session-config>
	<filter>
		<filter-name>CacheFilter</filter-name>
		<filter-class>filter.CacheFilter</filter-class>
		<async-supported>true</async-supported>
	</filter>
	<filter>
		<filter-name>EncodeFilter</filter-name>
		<filter-class>filter.EncodeFilter</filter-class>
		<async-supported>true</async-supported>
		<init-param>
			<param-name>encoding</param-name>
			<param-value>UTF-8</param-value>
		</init-param>
	</filter>
	<filter>
		<filter-name>SystemAccessFilter</filter-name>
		<filter-class>filter.SystemAccessFilter</filter-class>
		<async-supported>true</async-supported>
	</filter>
	<filter>
		<filter-name>LocalLogValidFilter</filter-name>
		<filter-class>filter.LocalLogValidFilter</filter-class>
		<async-supported>true</async-supported>
	</filter>
	<filter>
		<filter-name>LogValidFilter</filter-name>
		<filter-class>filter.LogValidFilter</filter-class>
		<async-supported>true</async-supported>
	</filter>
	<servlet>
		<servlet-name>UserServlet</servlet-name>
		<servlet-class>servlet.UserServlet</servlet-class>
		<async-supported>true</async-supported>
	</servlet>
	<servlet>
		<servlet-name>GradeQueryServlet</servlet-name>
		<servlet-class>servlet.GradeQueryServlet</servlet-class>
		<async-supported>true</async-supported>
	</servlet>
	<servlet>
		<servlet-name>ExamServlet</servlet-name>
		<servlet-class>servlet.ExamServlet</servlet-class>
		<async-supported>true</async-supported>
	</servlet>
	<servlet>
		<servlet-name>TimeTableServlet</servlet-name>
		<servlet-class>servlet.TimeTableServlet</servlet-class>
		<async-supported>true</async-supported>
	</servlet>
	<servlet>
		<servlet-name>RoomQueryServlet</servlet-name>
		<servlet-class>servlet.RoomQueryServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ClassTimeTableServlet</servlet-name>
		<servlet-class>servlet.ClassTimeTableServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>TeacherInfoServlet</servlet-name>
		<servlet-class>servlet.TeacherInfoServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ManageServlet</servlet-name>
		<servlet-class>servlet.ManageServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AsyncDispatchServlet</servlet-name>
		<servlet-class>servlet.AsyncDispatchServlet</servlet-class>
		<async-supported>true</async-supported>
	</servlet>
	<filter-mapping>
		<filter-name>EncodeFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>SystemAccessFilter</filter-name>
		<url-pattern>*.ym</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>CacheFilter</filter-name>
		<url-pattern>/js/*</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>CacheFilter</filter-name>
		<url-pattern>/image/*</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>CacheFilter</filter-name>
		<url-pattern>/css/*</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>LocalLogValidFilter</filter-name>
		<url-pattern>/grade.ym</url-pattern>
		<url-pattern>/timeTable.ym</url-pattern>
		<url-pattern>/exam.ym</url-pattern>
		<url-pattern>/classTimeTable.ym</url-pattern>
		<url-pattern>/room.ym</url-pattern>
		<url-pattern>/monitor.ym</url-pattern>
		<url-pattern>/teacherInfo.ym</url-pattern>
		<dispatcher>FORWARD</dispatcher>
		<dispatcher>INCLUDE</dispatcher>
		<dispatcher>REQUEST</dispatcher>
		<dispatcher>ASYNC</dispatcher>
	</filter-mapping>
	<filter-mapping>
		<filter-name>LogValidFilter</filter-name>
		<url-pattern>/_async_newGrade.ym</url-pattern>
		<url-pattern>/_async_newTimeTable.ym</url-pattern>
		<url-pattern>/_async_newExam.ym</url-pattern>
		<dispatcher>FORWARD</dispatcher>
		<dispatcher>INCLUDE</dispatcher>
		<dispatcher>REQUEST</dispatcher>
		<dispatcher>ASYNC</dispatcher>
	</filter-mapping>
	<servlet-mapping>
		<servlet-name>AsyncDispatchServlet</servlet-name>
		<url-pattern>/login.ym</url-pattern>
		<url-pattern>/newGrade.ym</url-pattern>
		<url-pattern>/newTimeTable.ym</url-pattern>
		<url-pattern>/newExam.ym</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UserServlet</servlet-name>
		<url-pattern>/_async_login.ym</url-pattern>
		<url-pattern>/logout.ym</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>GradeQueryServlet</servlet-name>
		<url-pattern>/grade.ym</url-pattern>
		<url-pattern>/_async_newGrade.ym</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ExamServlet</servlet-name>
		<url-pattern>/exam.ym</url-pattern>
		<url-pattern>/_async_newExam.ym</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>TimeTableServlet</servlet-name>
		<url-pattern>/timeTable.ym</url-pattern>
		<url-pattern>/_async_newTimeTable.ym</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>RoomQueryServlet</servlet-name>
		<url-pattern>/room.ym</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ClassTimeTableServlet</servlet-name>
		<url-pattern>/classTimeTable.ym</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>TeacherInfoServlet</servlet-name>
		<url-pattern>/teacherInfo.ym</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ManageServlet</servlet-name>
		<url-pattern>/manage.ym</url-pattern>
		<url-pattern>/monitor.ym</url-pattern>
	</servlet-mapping>
</web-app>